include "divio" : *;

extern "C" {
    func _getch() -> i32;
}

func main(argc: i32, argv[]: i8*) -> i32 {
    let data: i8* = "Merhaba!";
    let file: FILE* = fopen("test.txt", "w");
    fwrite(data, 1, sizeof(data), file);
    fclose(file);
    printf("press any key to continue...");
    _getch();
    remove("test.txt");
    ret 0;
}