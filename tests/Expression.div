extern "C" {
    func SimpleExpression(x: f64) -> f64 {
        ret x + 2.32f64;
    }

    func ComplexExpression(x: f64) -> f64 {
        ret 8.0f64 + (x + 1.0f64) * 2.5f64;
    }

    func SimpleLogicalExpression(x: bool, y: bool) -> bool {
        ret x && y;
    }

    func ComplexLogicalExpression(x: bool, y: bool, z: bool) -> bool {
        ret x && y || z;
    }

    func SimpleBitwiseExpression(x: i64, y: i64) -> i64 {
        ret x & y;
    }

    func ComplexBitwiseExpression(x: i64, y: i64, z: i64) -> i64 {
        ret x << y | z & 0xFFi64;
    }
}