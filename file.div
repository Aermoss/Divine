include "divio", "divmem", "divstr" : *;

extern "C" {
    func _getch() -> i32;
}

func main(argc: i32, argv[]: i8*) -> i32 {
    let data: i8* = "Esenlikler!";
    let file: FILE* = fopen("test.txt", "w");
    fwrite(data, 1u64, strlen(data) as u64, file);
    fclose(file);
    printf("press any key to continue...");
    _getch();
    remove("test.txt");
    ret 0;
}