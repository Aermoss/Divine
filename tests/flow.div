extern "C" {
    func IfBlockTest(x: i32) -> i64 {
        if x >= 5i32 {
            ret 10i64;
        } else if x == 3i32 {
            ret 7i64;
        } else {
            ret 5i64;
        }
    }

    func WhileBlockTest(mut x: i32) -> i64 {
        let mut i: i64 = 0i64;

        while x != 0i32 {
            i += 2i64;
            x -= 1i32;
        }

        ret i;
    }
}