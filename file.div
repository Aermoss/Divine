include "divio", "divmem", "divstr" : *;

extern "C" {
    func _getch() -> i32;
}

func main(argc: i32, argv[]: *const i8) -> i32 {
    let data: *const i8 = "Esenlikler!";
    let file: *const FILE = fopen("test.txt", "w");
    fwrite(data, 1u64, strlen(data), file);
    fclose(file);
    printf("press any key to continue...");
    _getch();
    remove("test.txt");
    ret 0i32;
}