include "divio", "divstr", "divvec" : *;

func main(argc: i32, argv[]: i8*) -> i32 {
    let a: i32 = 32;
    let b: i32 = 12;
    let c: i32 = a + b;
    a = 55;

    printf("Hello, World! %d + %d = %d\n", a, b, c);

    let i: i32 = 0;

    while i < 10 {
        i += 1;

        if i == 5 {
            continue;
        }

        if i == 8 {
            break;
        }

        printf("%d\n", i);
    }

    let test: i32 = 0;
    let ref: i32& = test;
    ref = 42;

    printf("test = %d\n", test);

    let str: String;
    printf("size = %d, data = %s\n", str.Size(), str.Data());
    str = "Hello, World!";
    printf("size = %d, data = %s\n", str.Size(), str.Data());
    str += "\nGoodbye, World!";
    printf("size = %d, data = %s\n", str.Size(), str.Data());

    let vec: Vector<i32>;
    vec.PushBack(1);

    printf("size = %d\n", vec.Size());
    ret 0;
}